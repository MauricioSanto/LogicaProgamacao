algoritmo "Condicional SE juntamente do SENAO"
var n1: inteiro
    n2: inteiro
inicio
    escreval("Insira o primeiro número:")
    leia (n1)
    escreval(n1)
    escreval("Insira o segundo número:")
    leia (n2)
    escreval(n2)
    se n1 > n2 faca
        escreva("O número ", n1," é maior que ", n2)
    senao
        escreva("O número ", n1,"  não é maior que ", n2)
    fimse

fimalgoritmo